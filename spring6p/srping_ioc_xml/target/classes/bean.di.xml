<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="book" class="com.alana.spring6.iocxml.di.Book">
        <property name="name" value="非暴力沟通"></property>
        <property name="author" value="马歇尔"></property>

    </bean>

    <bean id="bookcon" class="com.alana.spring6.iocxml.di.Book">
        <constructor-arg value="人民的名义"></constructor-arg>
        <constructor-arg value="周梅森"></constructor-arg>
    </bean>

</beans>